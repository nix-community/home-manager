[Install]
WantedBy=default.target

[Service]
BusName=org.mpris.MediaPlayer2.mpd.test-instance
ExecStart=@mpd-mpris@/bin/mpd-mpris -host example.com -instance-name test-instance -port 1234
Restart=on-failure
RestartSec=5s
Type=dbus

[Unit]
Description=mpd-mpris: An implementation of the MPRIS protocol for MPD
