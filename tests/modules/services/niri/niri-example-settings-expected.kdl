// early config
// Automatically generated by home-manager from `wayland.windowManager.niri.settings`
workspace "chat"
workspace "dev"
output "eDP-1" {
	scale 2.000000
}
window-rule {
	match app-id="firefox" at-startup=true
	open-on-workspace "dev"
}
binds {
	Mod+H {
		focus-column-left
	}
	Mod+Return hotkey-overlay-title="Open a Terminal" {
		spawn "ghostty"
	}
	XF86AudioRaiseVolume allow-when-locked=true {
		spawn "wpctl" "set-volume" "@DEFAULT_AUDIO_SINK@" "5%+"
	}
}
input {
	touchpad {
		tap
	}
}
layout {
	gaps 8
	preset-column-widths {
		proportion 0.333330
		proportion 0.500000
	}
	shadow {
		draw-behind-window true
		offset x=0 y=5
	}
}
prefer-no-csd
screenshot-path "~/Screenshots/%Y-%m-%d %H-%M-%S.png"
